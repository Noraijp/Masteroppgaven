issn = {0168-9002},
journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
keywords = {Deuteron breakup,Foil activation analysis,Neutron spectroscopy,Time-of-flight},
pages = {359--366},
title = {{Neutron Spectroscopy for pulsed beams with frame overlap using a double time-of-flight technique}},
url = {http://www.sciencedirect.com/science/article/pii/S0168900217310215},
volume = {877},
year = {2018}
