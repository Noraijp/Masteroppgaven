journal = {Nuclear Instruments and Methods in Physics Research Section B: Beam Interactions with Materials and Atoms},
keywords = {DD neutron generator,Indium,Medical isotope production,Ratio activation,Scandium (Sc) and copper (Cu) radioisotopes,Theranostics},
