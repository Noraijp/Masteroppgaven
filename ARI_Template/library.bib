@article{Stupegia1968,
abstract = {Neutron capture cross sections have been measured as a function of neutron energy between about 5 keV and 3 MeV for 41K, 55Mn, 85Rb, 89Y, 98Mo, 139La, 141Pr, 158Gd, 170Er, 175Lu and 176Yb, using the activation method. The data have been compared with calculations based upon the statistical compound model of nuclear reactions. For nuclei which are not deformed, one can predict the correct shapes of the cross-section curves from a few keV up to the energy at which the target levels are known. Predicting the absolute values depends sensitively upon the choice of $\Lambda$$\gamma$/Dobs, for which parameter accurate values are not always available. For the deformed nuclei, statistical theory in combination with the spherical optical model does not correctly predict the shapes.},
