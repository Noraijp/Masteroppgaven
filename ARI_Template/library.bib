issn = {0168-583X},
journal = {Nuclear Instruments and Methods in Physics Research Section B: Beam Interactions with Materials and Atoms},
keywords = {Cross-section measurement,Niobium,Physical yield,Thin layer activation},
number = {19},
pages = {3364--3374},
title = {{Investigation of proton induced reactions on niobium at low and medium energies}},
url = {http://www.sciencedirect.com/science/article/pii/S0168583X09008337},
