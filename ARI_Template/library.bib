doi = {10.1103/PhysRevC.49.2136},
issn = {0556-2813},
journal = {Physical Review C},
month = {apr},
number = {4},
pages = {2136--2141},
title = {{Global optical potentials for emitted alpha particles}},
url = {https://journals.aps.org/prc/pdf/10.1103/PhysRevC.49.2136 https://link.aps.org/doi/10.1103/PhysRevC.49.2136},
volume = {49},
year = {1994}
}
