keywords = {Bubble breakup,Effervescent atomization,Spray,Ultrasound},
month = {mar},
number = {3},
pages = {305--315},
publisher = {Elsevier B.V.},
title = {{Effect of ultrasound on bubble breakup within the mixing chamber of an effervescent atomizer}},
url = {http://dx.doi.org/10.1016/j.cep.2011.01.006 https://linkinghub.elsevier.com/retrieve/pii/S0255270111000213},
