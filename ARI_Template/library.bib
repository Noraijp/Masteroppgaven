publisher = {Elsevier},
title = {{Effect of ultrasound on mass transfer during electrodeposition for electrodes separated by a narrow gap}},
url = {http://dx.doi.org/10.1016/j.ces.2014.03.026 https://linkinghub.elsevier.com/retrieve/pii/S0009250914001377},
