@article{Steyn1990,
abstract = {Abstract Excitation functions for the production of 52Fe in the bombardment of Mn and Ni with protons were measured from threshold up to 200 MeV. Production rates of 52Fe as well as of its 55Fe and 59Fe impurities were also measured in specific energy windows ranging up to 100 MeV. The agreement with previous measurements, where available, is reasonably good, except that considerably higher 55Fe contamination levels than those recently reported below 70 MeV were obtained in the case of Ni. The experimental results were compared with theoretical calculations by means of the computer code ALICE/85/300. Overall agreement to within a factor of two was obtained, and the usefulness of the code in planning a radioisotope production process was demonstrated. Finally, practical production rates and impurity levels, obtained with a 66 MeV proton beam at high intensities (∼50 $\mu$A), are reported.},
author = {Steyn, G.F. and Mills, S.J. and Nortier, F.M. and Simpson, B.R.S. and Meyer, B.R.},
doi = {10.1016/0883-2889(90)90197-O},
