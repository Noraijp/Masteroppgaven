@article{Gibson2006,
abstract = {Radionuclide Therapy (RNT) and Radioimmunotherapy (RIT) are potentially of great interest for cancer therapy. In many therapeutic applications alpha emitters should be much more effective than already-approved beta emitters due to the short range and high linear energy transfer of alpha particles. 213 Bi is an important alpha emitter already used in clinical trials but the half-life of this radioisotope is short (46 minutes) and so its use is limited for certain therapies. 211 At is potentially very interesting for medical purposes because of its longer half-life of 7.2 hours, and suitable decay scheme. We have studied the cyclotron-based production of 211 At via the reaction 209 Bi($\alpha$, 2n), this production route probably being the most promising in the long term. The energy dependence of thick target yields and the reaction cross sections for the production of 211 At and 210 At were determined and found to be in good agreement with literature. The best energy to produce 211 At is 28-29 MeV. The possible production of the undesired, highly radiotoxic, and long-lived alpha-emitting 210 Po (138.38 days), which is produced from decay of 210 At, is also discussed.},
author = {Alfarano, A and Abbas, K and Holzwarth, U and Bonardi, M and Groppi, F and Alfassi, Z and Menapace, E and Gibson, P N},
doi = {10.1088/1742-6596/41/1/009},
file = {:media/andrew/Storage/Mendeley/Alfarano et al. - 2006 - Thick target yield measurement of 211 At through the nuclear reaction 209 Bi($\alpha$, 2n).pdf:pdf},
issn = {1742-6588},
journal = {Journal of Physics: Conference Series},
month = {may},
number = {1},
pages = {115--122},
title = {{Thick target yield measurement of 211 At through the nuclear reaction 209 Bi($\alpha$, 2n)}},
url = {http://stacks.iop.org/1742-6596/41/i=1/a=009?key=crossref.4dbba9c8800b7593b95b024adab2d6e8},
volume = {41},
year = {2006}
}
