file = {:media/andrew/Storage/Mendeley/Shimizu et al. - 2004 - Measurements of activation cross sections of (n, p) and (n, $\alpha$) reactions with d-D neutrons in the energy range.pdf:pdf},
issn = {0306-4549},
journal = {Annals of Nuclear Energy},
keywords = {Activation method,Cross section,d-D Neutrons},
number = {9},
pages = {975--990},
title = {{Measurements of activation cross sections of (n, p) and (n, $\alpha$) reactions with d-D neutrons in the energy range of 2.1-3.1 MeV}},
