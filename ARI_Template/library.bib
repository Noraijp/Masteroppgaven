issn = {0168-583X},
journal = {Nuclear Instruments and Methods in Physics Research Section B: Beam Interactions with Materials and Atoms},
keywords = {70MeV proton beam,ARRONAX cyclotron,Copper,Nickel,Stacked-foil technique,TALYS 1.6,Titanium},
number = {Supplement C},
pages = {191--212},
title = {{New excitation functions for proton induced reactions on natural titanium, nickel and copper up to 70MeV}},
