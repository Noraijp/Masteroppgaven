pages = {823--832},
title = {{A comparative study on the separation of radioyttrium from Sr- and Rb-targets via ion-exchange and solvent extraction techniques, with special reference to the production of no-carrier-added 86Y, 87Y and 88Y using a cyclotron}},
url = {https://doi.org/10.1007/s10967-008-7407-0 http://link.springer.com/10.1007/s10967-008-7407-0},
volume = {279},
year = {2009}
}
