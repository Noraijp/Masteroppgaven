issn = {0167-8140},
journal = {Radiotherapy and Oncology},
keywords = {First in man,Prompt gamma imaging,Proton therapy,Range verification,Slit camera},
number = {2},
pages = {232--237},
title = {{First clinical application of a prompt gamma based in vivo proton range verification system}},
url = {http://www.sciencedirect.com/science/article/pii/S0167814016000074},
volume = {118},
