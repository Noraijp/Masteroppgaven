keywords = {Monte Carlo method,Nuclear data,Nuclear reaction,Secondary particle energy spectra,Statistical model},
month = {may},
number = {5},
pages = {462--469},
publisher = {Taylor & Francis},
title = {{Monte Carlo Simulation for Particle and $\gamma$-Ray Emissions in Statistical Hauser-Feshbach Model}},
url = {http://www.tandfonline.com/doi/abs/10.1080/18811248.2010.9711637},
volume = {47},
year = {2010}
