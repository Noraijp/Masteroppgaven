@article{Schaffer2017,
abstract = {Effective use of the ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn001.gif] {${}^{225}{\rm Ac}$} decay chain in targeted internal radioimmunotherapy requires the retention of both ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn002.gif] {${}^{225}{\rm Ac}$} and progeny isotopes at the target site. Imaging-based pharmacokinetic tests of these pharmaceuticals must therefore separately yet simultaneously image multiple isotopes that may not be colocalized despite being part of the same decay chain. This work presents feasibility studies demonstrating the ability of a microSPECT/CT scanner equipped with a high energy collimator to simultaneously image two components of the ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn003.gif] {${}^{225}{\rm Ac}$} decay chain: ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn004.gif] {${}^{221}{\rm Fr}$} (218 keV) and ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn005.gif] {${}^{213}{\rm Bi}$} (440 keV). Image quality phantoms were used to assess the performance of two collimators for simultaneous ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn006.gif] {${}^{221}{\rm Fr}$} and ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn007.gif] {${}^{213}{\rm Bi}$} imaging in terms of contrast and noise. A hotrod resolution phantom containing clusters of thin rods with diameters ranging between 0.85 and 1.70 mm was used to assess resolution. To demonstrate ability to simultaneously image dynamic ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn008.gif] {${}^{221}{\rm Fr}$} and ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn009.gif] {${}^{213}{\rm Bi}$} activity distributions, a phantom containing a ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn010.gif] {${}^{213}{\rm Bi}$} generator from ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn011.gif] {${}^{225}{\rm Ac}$} was imaged. These tests were performed with two collimators, a high-energy ultra-high resolution (HEUHR) collimator and an ultra-high sensitivity (UHS) collimator. Values consistent with activity concentrations determined independently via gamma spectroscopy were observed in high activity regions of the images. In hotrod phantom images, the HEUHR collimator resolved all rods for both ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn012.gif] {${}^{221}{\rm Fr}$} and ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn013.gif] {${}^{213}{\rm Bi}$} images. With the UHS collimator, no rods were resolvable in ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn014.gif] {${}^{213}{\rm Bi}$} images and only rods ⩾1.3 mm were resolved in ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn015.gif] {${}^{221}{\rm Fr}$} images. After eluting the ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn016.gif] {${}^{213}{\rm Bi}$} generator, images accurately visualized the reestablishment of transient equilibrium of the ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn017.gif] {${}^{225}{\rm Ac}$} decay chain. The feasibility of evaluating the pharmacokinetics of the ##IMG## [http://ej.iop.org/images/0031-9155/62/11/4406/pmbaa6a99ieqn018.gif] {${}^{225}{\rm Ac}$} decay chain in vivo has been demonstrated. This presented method requires the use of a high-performance high-energy collimator.},
author = {Robertson, A K H and Ramogida, C F and Rodr{\'{i}}guez-Rodr{\'{i}}guez, C and Blinder, Stephan and Kunz, Peter and Sossi, Vesna and Schaffer, Paul},
doi = {10.1088/1361-6560/aa6a99},
file = {:media/andrew/Storage/Mendeley/Robertson et al. - 2017 - Multi-isotope SPECT imaging of the 225 Ac decay chain feasibility studies.pdf:pdf},
issn = {0031-9155},
journal = {Physics in Medicine and Biology},
month = {jun},
number = {11},
pages = {4406--4420},
title = {{Multi-isotope SPECT imaging of the 225 Ac decay chain: feasibility studies}},
url = {http://stacks.iop.org/0031-9155/62/i=11/a=4406 http://stacks.iop.org/0031-9155/62/i=11/a=4406?key=crossref.3db33e6e6d06b8730492ae90c789a2f2},
volume = {62},
year = {2017}
}
