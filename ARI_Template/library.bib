issn = {14397641},
journal = {ChemPhysChem},
keywords = {EPR spectroscopy,gadolinium,manganese,pulsed electron???electron double resonance,spin labels},
month = {jul},
number = {13},
pages = {2066--2078},
title = {{A Bis-Manganese(II)-DOTA Complex for Pulsed Dipolar Spectroscopy}},
url = {http://doi.wiley.com/10.1002/cphc.201600234},
volume = {17},
year = {2016}
}
