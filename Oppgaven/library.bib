issn = {1742-6588},
journal = {Journal of Physics: Conference Series},
month = {sep},
number = {1},
pages = {012028},
title = {{Pygmy dipole resonance}},
url = {http://stacks.iop.org/1742-6596/533/i=1/a=012028?key=crossref.0e225b7b68f0e1a5fd82dfdb842a9235},
