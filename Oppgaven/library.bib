month = {jan},
pages = {31--49},
publisher = {Elsevier Inc.},
title = {{Nuclear data for production and medical application of radionuclides: Present status and future needs}},
url = {http://dx.doi.org/10.1016/j.nucmedbio.2016.08.016 https://linkinghub.elsevier.com/retrieve/pii/S0969805116302190},
