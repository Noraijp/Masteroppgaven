@article{Caserta2018,
abstract = {{\textcopyright} 2018 by The American Society of Hematology. As a growing number of patients with multiple myeloma (MM) respond to upfront therapies while eventually relapsing in a time frame that is often unpredictable, attention has increasingly focused on developing novel diagnostic criteria to also account for disease dissemination. Positron emission tomography/computed tomography (PET/CT) is often used as a noninvasive monitoring strategy to assess cancer cell dissemination, but because the uptake of the currently used radiotracer 18fluorodeoxyglucose (18F-FDG) is a function of the metabolic activity of both malignant and nonmalignant cells, the results frequently lack sufficient specificity. Radiolabeled antibodies targeting MM tissue may detect disease irrespective of cell metabolism. Hence, we conjugated the clinically significant CD38-directed human antibody daratumumab (Darzalex [Dara]) to the DOTA chelator and labeled it with the positron-emitting radionuclide copper 64 (64Cu; 64Cu-DOTA-Dara). Here, we show that 64Cu-DOTA-Dara can efficiently bind CD38 on the surface ofMMcells and was mainly detected in the bones associated with tumor in a MM murine model. We also show that PET/CT based on 64Cu-DOTA-Dara displays a higher resolution and specificity to detectMMcell dissemination than does 18F-FDG PET/CT and was even more sensitive than were bioluminescence signals. We therefore have supporting evidence for using 64Cu-DOTA-Dara as a novel imaging agent for MM.},
author = {Caserta, Enrico and Chea, Junie and Minnix, Megan and Poku, Erasmus K. and Viola, Domenico and Vonderfecht, Steven and Yazaki, Paul and Crow, Desiree and Khalife, Jihane and Sanchez, James F. and Palmer, Joycelynne M. and Hui, Susanta and Carlesso, Nadia and Keats, Jonathan and Kim, Young and Buettner, Ralf and Marcucci, Guido and Rosen, Steven and Shively, John and Colcher, David and Krishnan, Amrita and Pichiorri, Flavia},
doi = {10.1182/blood-2017-09-807263},
file = {:Users/Nora/Documents/UiO/Masteroppgaven/Papers/Copper 64–labeled daratumumab as a PET$\backslash$:CT imaging tracer for multiple myeloma.pdf:pdf},
issn = {0006-4971},
journal = {Blood},
month = {feb},
number = {7},
pages = {741--745},
title = {{Copper 64–labeled daratumumab as a PET/CT imaging tracer for multiple myeloma}},
url = {http://www.bloodjournal.org/lookup/doi/10.1182/blood-2017-09-807263},
volume = {131},
year = {2018}
