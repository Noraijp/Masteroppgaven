@article{Santry1973a,
abstract = {The activation method was used to determine thermal neutron activation cross sections, $\sigma$ 0 , and resonance integrals, RI. Values obtained were $\sigma$ 0 = 47.5 ± 2.4 mb, RI = 218 ± 10 mb for 94 Zr and $\sigma$ 0 = 22.9 ± 1.0 mb, RI = 5.20 ± 0.54 b for 96 Zr. Calculations were based on the Westcott convention and the known cross sections for the 59 Co and 197 Au neutron flux monitors. Measurements confirmed the existence of a p-wave neutron resonance capture for 96 Zr. The half-life of 97 Zr was measured as 16.90 ± 0.05 h.},
author = {Santry, D. C. and Werner, R. D.},
