title = {{The Absorption and Translocation of Lead by Plants. A Contribution to the Application of the Method of Radioactive Indicators in the Investigation of the Change of Substance in Plants}},
url = {http://www.degruyter.com/view/books/harvard.9780674366701/harvard.9780674366701.c26/harvard.9780674366701.c26.xml},
